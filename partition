#include <stdio.h>
void main()
{
    int w,n,i=1,j=1,k=1;
    scanf("%d %d",&w,&n);
    for(j=1; j<=(w*n)+(n+1); j++)
    {
        printf("*");
    }
    printf("\n");
    for(k=1; k<=n; k++)
    {
        printf("*");
        for(i=1; i<=w; i++)
        {
            printf(" ");
        }
    }
    printf("*");
    printf("\n");
    for(j=1; j<=(w*n)+(n+1); j++)
    {
        printf("*");
    }
}
